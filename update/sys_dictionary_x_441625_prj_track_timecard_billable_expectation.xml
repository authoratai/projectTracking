        <audit>false</audit>
        <calculation><![CDATA[(function calculatedFieldValue(current) {

	var bill = current.billable;
	var pct = 0;
	var tw = 0;
	var grTw = new GlideRecord('x_441625_prj_track_time_worked');
	grTw.addQuery('timecard',current.sys_id);
	grTw.addQuery('billable', true);
	grTw.query();
	while (grTw.next()) {
		tw = tw + grTw.hours;
	}
	pct = (tw * 100) / bill;
	return pct;

})(current);]]></calculation>
